# Exemplo de arquivo .env
# Copie este arquivo para .env e preencha com seus valores reais

# Configurações da aplicação
VITE_APP_NAME=VacaFácil
VITE_APP_VERSION=2.0.0

# URLs da API (quando implementar o backend)
VITE_API_BASE_URL=http://localhost:8000
VITE_API_TIMEOUT=10000

# Configurações de autenticação
VITE_JWT_SECRET=<JWT_SECRET_KEY>
VITE_JWT_EXPIRES_IN=24h

# Configurações de upload
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/webp

# Configurações de notificação
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_VAPID_PUBLIC_KEY=<VAPID_PUBLIC_KEY>

# Configurações de analytics (opcional)
VITE_GOOGLE_ANALYTICS_ID=<GA_TRACKING_ID>

# Configurações de pagamento (quando implementar)
VITE_STRIPE_PUBLIC_KEY=<STRIPE_PUBLIC_KEY>
VITE_PAYPAL_CLIENT_ID=<PAYPAL_CLIENT_ID>

# Configurações de desenvolvimento
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info